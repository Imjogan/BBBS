{
  "extends": ["airbnb-base", "prettier"],
  "rules": {
    "no-underscore-dangle": ["error", { "allow": ["_id"] }],
    "import/no-extraneous-dependencies": ["error", { "devDependencies": true }],
    "no-unused-vars": "warn",
    "arrow-body-style": "warn",
    "import/no-named-as-default": "warn"
  }
}
